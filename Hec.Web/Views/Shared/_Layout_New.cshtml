<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Home Energy Calculator</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">

    <!-- Web Fonts  -->
    <link href="http://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700,800%7CShadows+Into+Light" rel="stylesheet" type="text/css">

    <!--NEW HEC-->
    <link rel="stylesheet" href="@Url.Content("~/bootstrap537/css/bootstrap.min.css")">
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/css/jcarousel.responsive.css")">
    <link rel="stylesheet" href="@Url.Content("~/css/hec.css")">
    <link rel="stylesheet" href="@Url.Content("~/css/hec-override.css")">
    <link rel="stylesheet" href="@Url.Content("~/css/responsive.css")">

    <!-- Theme CSS -->
    <link rel="stylesheet" href="@Url.Content("~/css/theme-new.css")">
    <link rel="stylesheet" href="@Url.Content("~/css/theme-elements-new.css")">
    <link rel="stylesheet" href="@Url.Content("~/css/theme-animate.css")">

    <!-- Skin CSS -->
    <link rel="stylesheet" href="@Url.Content("~/css/skins/default.css")">

    <!-- Current Page CSS -->
    <link rel="stylesheet" href="@Url.Content("~/vendor/rs-plugin/css/settings.css")" media="screen">
    <link rel="stylesheet" href="@Url.Content("~/vendor/circle-flip-slideshow/css/component.css")" media="screen">

    <!-- Theme Custom CSS -->
    <link rel="stylesheet" href="@Url.Content("~/Content/font-awesome.min.css")">


    <!-- Head Libs -->
    <script src="@Url.Content("~/vendor/modernizr/modernizr.js")"></script>
    <style>
        ul.social-icons li.digg {
            background-position: 0 0;
        }

            ul.social-icons li.digg:hover {
                background-position: 0 -30px;
            }

        ul.social-icons li.dribbble {
            background-position: 0 -60px;
        }

            ul.social-icons li.dribbble:hover {
                background-position: 0 -90px;
            }

        ul.social-icons li.login {
            background: #fff url('../images/login-icon.png') no-repeat center center;
        }

            ul.social-icons li.login:hover {
                background: #FBE838 url('../images/login-icon.png') no-repeat center center;
            }

        .thumb_v1 {
            display: block;
            padding: 10px;
            background-color: rgba(255,255,255,0.8);
            width: 250px;
            height: 80px;
            border: 1px solid #dddddd;
        }

    </style>
</head>
<body>

    @if (!String.IsNullOrEmpty(ViewBag.ErrorMessage))
    {
        <div class="alert alert-danger">
            @Html.Raw(ViewBag.ErrorMessage)
        </div>
    }
    @if (!String.IsNullOrEmpty(ViewBag.InfoMessage) || !String.IsNullOrEmpty(TempData["InfoMessage"] as string))
    {
        <div class="alert alert-info">
            @Html.Raw(ViewBag.InfoMessage ?? TempData["InfoMessage"])
        </div>
    }


<div class="body">


    @RenderBody()

    <footer id="footer" style="background: #060606 !important;margin-top:0px !important">

        <div class="footer-copyright">
            <div class="container py-2">
                <div class="row">
                    <div class="col-md-2 d-flex align-items-center justify-content-center justify-content-lg-start mb-2 mb-lg-0">
                        <a href="index.html" class="logo pe-0 pe-lg-3">
                            <img alt="HEC TNB" src="/Images/logos/tnbwhite.png" width="120" data-plugin-options="{'appearEffect': 'fadeIn'}">
                        </a>
                    </div>
                    <div class="col-md-6 d-flex ">
                        <p style="color:#fff">© Copyright 2017 - 2025. Tenaga Nasional Berhad. All Rights Reserved.</p>
                    </div>
                    <div class="col-md-4 d-flex align-content-end justify-content-end">
                        <nav id="sub-menu">
                            <ul>
                                <li><i class="fas fa-angle-right"></i><a href="/Content/HEC2-Training-Manual-User-v1.pdf" style="color:#aaa !important;">Guideline</a></li>
                                <li><i class="fas fa-angle-right"></i><a href="/Home/Sitemap" style="color:#aaa !important;">Sitemap</a></li>
                                <li><i class="fas fa-angle-right"></i><a href="https://www.tnb.com.my/contact-us/" style="color:#aaa !important;">Contact</a>&gt;</li>
                            </ul>
                        </nav>
                    </div>
                </div>
            </div>
        </div>
    </footer>

</div>

    <!--NEW HEC-->
    <script src="@Url.Content("~/vendor/jquery/jquery371.min.js")"></script>
    <script src="@Url.Content("~/vendor/jquery.appear/jquery.appear.js")"></script>
    <script src="@Url.Content("~/vendor/jquery.easing/jquery.easing.js")"></script>


    @*<script src="@Url.Content("~/vendor/bootstrap/bootstrap.js")"></script>*@

    @*<script src="@Url.Content("~/Scripts/jquery.min.js")"></script>*@
    <script src="@Url.Content("~/bootstrap537/js/bootstrap.min.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/jquery.jcarousel.min.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/jcarousel.responsive.js")"></script>
    <script src="@Url.Content("~/Scripts/hec.js")"></script>

    <!-- Theme Base, Components and Settings -->
    <script src="@Url.Content("~/Scripts/theme-new.js")"></script>

    <!-- Specific Page Vendor and Views -->
    @*<script src="@Url.Content("~/vendor/rs-plugin/js/jquery.themepunch.tools.min.js")"></script>
        <script src="@Url.Content("~/vendor/rs-plugin/js/jquery.themepunch.revolution.min.js")"></script>*@
    <script src="@Url.Content("~/Scripts/jquery.themepunch.tools.min.js")"></script>
    <script src="@Url.Content("~/Scripts/jquery.themepunch.revolution.min.js")"></script>
    <script src="@Url.Content("~/vendor/circle-flip-slideshow/js/jquery.flipshow.js")"></script>




    <!-- Theme Custom -->
    @*<script src="@Url.Content("~/Scripts/custom.js")"></script>*@

    <!-- Theme Initialization Files -->
    <script src="@Url.Content("~/Scripts/theme.init-new.js")"></script>

    <!--END HEC-->
    @RenderSection("Scripts", required: false)

